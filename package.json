  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "eslint": "^4.4.1",
    "express": "^4.15.4",
    "jsonwebtoken": "^7.4.2",
    "morgan": "^1.8.2",
