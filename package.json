{
  "name": "hello-books",
  "version": "1.0.0",
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting books.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf /dist && babel ./dist --out-dir / --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/app.js",
    "s": "babel-watch app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekundayo-ab/hello-books.git"
  },
