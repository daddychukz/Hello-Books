{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": [0, { "forbid": ["object"] }]
    },
    "globals": {
      "browser": true,
      "node": true,
      "window": true,
      "document": true
    },
    "env": {
      "jquery": true,
      "commonjs": true,
      "es6": true
    }
}